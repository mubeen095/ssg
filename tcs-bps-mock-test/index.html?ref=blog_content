    var valselections = {};
    function valselect(planid, itemid, amount){
        valselections[planid] = [itemid, amount];
    }
    function ccapaynowval(planid, planamount){
        ccapaynow(planid, planamount);
    }
    function ccapaynowval2(radiogrpname, allplanids, allamounts){
        var index = $('input[name="'+radiogrpname+'"]').index($('input[name="'+radiogrpname+'"]:checked'));
        var planid = allplanids[index];
        var planamount = allamounts[index];
        ccapaynow(planid, planamount);
    }

