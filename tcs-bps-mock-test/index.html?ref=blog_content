        $(this).removeClass("in");
        $(this).removeClass("active");
    }
    else{
        $(this).addClass("in");
        $(this).addClass("active");
        $(this).find(".faq-answer").slideDown("slow");
    }
    $(this).find(".fa-solid").toggleClass('fa-plus');
    $(this).find(".fa-solid").toggleClass('fa-minus');
});
