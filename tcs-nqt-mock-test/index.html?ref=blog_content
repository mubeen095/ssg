 $(".menu-filter .menu-items").mouseenter(function () {
            $(".menu-filter .menu-items").removeClass("item-active");
            $(".menu-filter .menu-items .item-txt").removeClass("item-txt-active");
            $(this).addClass("item-active");
            $(this).children("span.item-txt").addClass("item-txt-active");
            $(".ccdcards").hide();
            var groupid = $(this).attr("id").split("-")[1];
            $(".ccd-" + groupid).show();
            if(cardsfull == 1){
                $("#cards-container-full").hide();
                $("#cards-container-6").show();
                cardsfull = 0;
            }
        });
