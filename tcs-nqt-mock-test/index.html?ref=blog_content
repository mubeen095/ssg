        const $testiTxt = $(this);
        const lineHeight = parseInt($testiTxt.css('line-height'));

        // Check if content exceeds three lines
        if ($testiTxt.prop('scrollHeight') > 3 * lineHeight) {
            $testiTxt.addClass('truncated');
            $testiTxt.css('max-height', 3 * lineHeight + 'px');

            // Append Read more button
            $testiTxt.after('<button class="read-more">Read more &gt;&gt;</button>');

            // Click event handler for Read more button
            $testiTxt.siblings('.read-more').on('click', function() {
                $(this).siblings('.testi-txt').toggleClass('truncated');
                if ($(this).siblings('.testi-txt').hasClass('truncated')) {
                    $(this).text('Read more >>');
                     $(this).siblings('.testi-txt').css('max-height', 3 * lineHeight + 'px');
                    $(".testi-card").css('height','225px');
                } else {
                    $(this).text('Read less >>');
                     $(this).siblings('.testi-txt').css('max-height', '');
                    $(".testi-card").css('height','auto');
                }
            });
        }
});*/
});
