function testscroll2(direction, no) {
    const $container = $('#takefreemock'+no+'-row-scroll');
    const $leftArrow = $('#takefreemock'+no+'-tabarrows-left');
    const $rightArrow = $('#takefreemock'+no+'-tabarrows-right');
    const scrollStep = 30;
    let scrollCompleted = 0;

    const slideVar = setInterval(function() {
        if (direction === 'left') {
            $container.scrollLeft($container.scrollLeft() - scrollStep);
        } else {
            $container.scrollLeft($container.scrollLeft() + scrollStep);
        }
        
        scrollCompleted += scrollStep;
        
        if (scrollCompleted >= 250) {
            clearInterval(slideVar);
        }

        updateTabArrowsVisibility();
    }, 50);

    function updateTabArrowsVisibility() {
        if ($container.scrollLeft() <= 0) {
            $leftArrow.hide();
        } else {
            $leftArrow.show();
        }

        if ($container.scrollLeft() + $container.width() >= $container.get(0).scrollWidth) {
            $rightArrow.hide();
        } else {
            $rightArrow.show();
        }
    }
}


